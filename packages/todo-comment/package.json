{
  "name": "eslint-plugin-todo-comment",
  "version": "1.4.1",
  "description": "This is a rule to encourage the entry of ticket URLs with TODO comments.",
  "main": "dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "test": "yarn vitest",
    "typecheck": "tsc --noEmit"
  },
  "author": "ToyB0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ToyB0x/eslint-plugins/issues"
  },
  "homepage": "https://github.com/ToyB0x/eslint-plugins",
  "volta": {
    "extends": "../../package.json"
  },
  "dependencies": {
    "@typescript-eslint/utils": "7.18.0"
  },
  "devDependencies": {
    "@types/node": "22.13.5",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@typescript-eslint/rule-tester": "7.18.0",
    "eslint": "8.57.1",
    "prettier": "3.5.2",
    "tsup": "8.4.0",
    "typescript": "5.7.3",
    "vitest": "3.0.7"
  }
}
